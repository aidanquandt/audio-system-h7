{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build (Debug)",
            "type": "shell",
            "command": "cmake",
            "args": [
                "--build",
                "cube/CM7/build/Debug",
                "-j8"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": ["$gcc"]
        },
        {
            "label": "Clean Build",
            "type": "shell",
            "command": "cmake",
            "args": [
                "--build",
                "cube/CM7/build/Debug",
                "--target",
                "clean"
            ],
            "problemMatcher": []
        },
        {
            "label": "Configure (Debug)",
            "type": "shell",
            "command": "cmake",
            "args": [
                "--preset",
                "Debug"
            ],
            "options": {
                "cwd": "${workspaceFolder}/cube/CM7"
            },
            "problemMatcher": []
        }
    ]
}
